{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js", // Change this to your entry file
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist" // Change this to your output directory
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html" // Change this if your routing is different
    }
  ]
}
